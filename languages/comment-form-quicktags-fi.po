#: /comment-form-quicktags.php:224
msgid "Comment Form Quicktags Options"
msgstr "Comment Form Quicktags Asetukset"

#: /comment-form-quicktags.php:109
msgid "Comment Form Quicktags"
msgstr "Comment Form Quicktags"

#: /comment-form-quicktags.php:120
msgid "Are you sure?"
msgstr "Oletko varma?"

#: /comment-form-quicktags.php:155
msgid "Settings"
msgstr "Asetukset"

#: /comment-form-quicktags.php:212
msgid "Options saved."
msgstr "Asetukset tallennettu."

#: /comment-form-quicktags.php:217
msgid "Options removed."
msgstr "Asetukset poistettu."

#: /comment-form-quicktags.php:225
msgid "Javascript is needed! "
msgstr "Tarvitaan Javascriptiä!"

#: /comment-form-quicktags.php:227
msgid "Tag Options"
msgstr "Tagien asetukset"

#: /comment-form-quicktags.php:229
msgid "Click a tag which you want to edit or input ID which you want to add."
msgstr "Paina tagia, jota haluat muokata tai syötä uuden tagin ID."

#: /comment-form-quicktags.php:230
msgid "Edit other field."
msgstr "Lisää tai muokkaa muut tiedot sopiviksi."

#: /comment-form-quicktags.php:231
msgid "Click Edit/Add button."
msgstr "Paina \"Muokaa/Lisää\"-nappia."

#: /comment-form-quicktags.php:232
msgid "Sort tags by dragging."
msgstr "Valitse tagien järjestys."

#: /comment-form-quicktags.php:233
msgid "After that, click Update Tags button to save."
msgstr "Sen jälkeen paina \"Päivitä tagit\"-nappia tallentaaksesi asetukset."

#: /comment-form-quicktags.php:235
#, php-format
msgid "<strong>[Info]</strong> Allowed tags in comments: <code class=\"tags\">%s</code>."
msgstr "<strong>[Tietoa]</strong> Sallitut tagit kommenteissa: <code class=\"tags\">%s</code>."

#: /comment-form-quicktags.php:236
msgid "<strong>[Note]</strong> There are special IDs: ed_link, ed_img and ed_close."
msgstr "<strong>[Muista]</strong> Nämä ovat erikoistunnuksia (IDeitä): ed_link, ed_img and ed_close."

#: /comment-form-quicktags.php:252
msgid "ID<span>*</span>"
msgstr "ID<span>*</span>"

#: /comment-form-quicktags.php:255
msgid "Display<span>*</span>"
msgstr "Nimi<span>*</span>"

#: /comment-form-quicktags.php:258
msgid "Start tag"
msgstr "Tagin alku"

#: /comment-form-quicktags.php:261
msgid "End tag"
msgstr "Tagin loppu"

#: /comment-form-quicktags.php:264
msgid "Access key"
msgstr "Avain"

#: /comment-form-quicktags.php:269
msgid "Edit/Add"
msgstr "Muokaa/Lisää"

#: /comment-form-quicktags.php:270
msgid "Delete"
msgstr "Poista"

#: /comment-form-quicktags.php:279
msgid "Update Tags"
msgstr "Päivitä tagit"

#: /comment-form-quicktags.php:284
#: /comment-form-quicktags.php:289
msgid "Remove options"
msgstr "Poista asetukset"

#: /comment-form-quicktags.php:285
msgid "You can remove the above options from the database. All the settings return to default."
msgstr "Pystyt aina poistamaan nämä asetukset tietokannastasi. Tällöin asetukset palaavat oletuksiinsa."

