msgid "Start tag"
msgstr "開始タグ"

#: comment-form-quicktags.php:388
msgid "End tag"
msgstr "終了タグ"

#: comment-form-quicktags.php:391
msgid "Access key"
msgstr "アクセスキー"

#: comment-form-quicktags.php:396
msgid "Edit/Add"
msgstr "編集/追加"

