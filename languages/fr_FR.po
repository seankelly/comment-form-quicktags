msgid "Comment Form Quicktags Options"
msgstr "Options pour Comment Form Quicktags"

#: comment-form-quicktags.php:211
msgid "Comment Form Quicktags"
msgstr "Comment Form Quicktags"

#: comment-form-quicktags.php:224
msgid "Are you sure?"
msgstr "Êtes-vous sûr?"

#: comment-form-quicktags.php:245
msgid "Settings"
msgstr "Paramètres"

#: comment-form-quicktags.php:329 comment-form-quicktags.php:339
msgid "Options saved."
msgstr "Options sauvegardées."

#: comment-form-quicktags.php:347
msgid "Options removed."
msgstr "Options supprimées."

#: comment-form-quicktags.php:355
msgid "Javascript is needed! "
msgstr "Le Javascript est nécessaire!"

#: comment-form-quicktags.php:357
msgid "Tag Options"
msgstr "Options des balises"

#: comment-form-quicktags.php:359
msgid "Click a tag which you want to edit or input ID which you want to add."
msgstr "Cliquez sur la balise que vous voulez éditer ou entrez une ID pour en ajouter une nouvelle."

#: comment-form-quicktags.php:360
msgid "Edit other field."
msgstr "Modifiez les autres champs."

#: comment-form-quicktags.php:361
msgid "Click Edit/Add button."
msgstr "Cliquez sur le bouton \"Editer/Ajouter\""

#: comment-form-quicktags.php:362
msgid "Sort tags by dragging."
msgstr "Changez l'emplacement des balises en les déplaçant."

#: comment-form-quicktags.php:363
msgid "After that, click Update Tags button to save."
msgstr "Après celà, cliquez sur \"Mettre à jour\" pour sauvegarder les changements."

#: comment-form-quicktags.php:365
#, php-format
msgid "<strong>[Info]</strong> Allowed tags in comments: <code class=\"tags\">%s</code>."
msgstr "<strong>[INFO]</strong> Balises autorisées dans les commentaires: <code class=\"tags\">%s</code>."

#: comment-form-quicktags.php:366
msgid "<strong>[Note]</strong> There are special IDs: ed_link, ed_img and ed_close."
msgstr "<strong>[NOTE]</strong> Il y a des ID spéciaux: ed_link, ed_img and ed_close."

#: comment-form-quicktags.php:384
msgid "ID<span>*</span>"
msgstr "ID"

#: comment-form-quicktags.php:387
